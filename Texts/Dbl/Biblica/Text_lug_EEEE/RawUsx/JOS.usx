        <verse number="20" style="v"/>
        <note caller="-" style="x">
          <char style="xo" closed="false">12:20 </char>
          <char style="xt" closed="false">
            <ref loc="JOS 11:1">Yos 11:1</ref>
          </char>
        </note>ne kabaka w’e Simuloni Meroni </cell>
      <cell style="tc2" align="start">omu,</cell>
    </row>
    <row style="tr">
      <cell style="tc1" align="start">ne kabaka w’e Akusafu </cell>
      <cell style="tc2" align="start">omu,</cell>
    </row>
    <row style="tr">
      <cell style="tc1" align="start">
        <verse number="21" style="v"/>n’ow’e Taanaki </cell>
      <cell style="tc2" align="start">omu,</cell>
    </row>
    <row style="tr">
      <cell style="tc1" align="start">n’ow’e Megiddo </cell>
      <cell style="tc2" align="start">omu,</cell>
    </row>
    <row style="tr">

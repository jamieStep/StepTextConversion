        <verse number="22" style="v"/>
        <note caller="-" style="x">
          <char style="xo" closed="false">12:22 a </char>
          <char style="xt" closed="false">
            <ref loc="JOS 19:37">Yos 19:37</ref>; <ref loc="JOS 20:7">20:7</ref>; <ref loc="JOS 21:32">21:32</ref> </char>
          <char style="xo" closed="false">b </char>
          <char style="xt" closed="false">
            <ref loc="1SA 15:12">1Sa 15:12</ref>
          </char>
        </note>n’ow’e Kedesi </cell>
      <cell style="tc2" align="start">omu,</cell>
    </row>
    <row style="tr">
      <cell style="tc1" align="start">ne kabaka w’e Yokuneamu ku Kalumeeri </cell>

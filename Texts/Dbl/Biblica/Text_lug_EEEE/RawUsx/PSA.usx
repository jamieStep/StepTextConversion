      <char style="xt" closed="false">
        <ref loc="PSA 89:7">Zab 89:7</ref> </char>
      <char style="xo" closed="false">c </char>
      <char style="xt" closed="false">
        <ref loc="PSA 95:3">Zab 95:3</ref>
      </char>

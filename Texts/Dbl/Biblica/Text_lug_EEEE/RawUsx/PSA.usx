      <char style="xo" closed="false">135:11 a </char>
      <char style="xt" closed="false">
        <ref loc="NUM 21:21">Kbl 21:21</ref> </char>
      <char style="xo" closed="false">b </char>
      <char style="xt" closed="false">
        <ref loc="JOS 12:7-24">Yos 12:7-24</ref>
      </char>

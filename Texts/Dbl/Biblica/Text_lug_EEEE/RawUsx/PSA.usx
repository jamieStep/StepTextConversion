  <para style="b"/>
  <para style="q1">
    <verse number="13" style="v"/>
    <note caller="-" style="x">
      <char style="xo" closed="false">135:13 a </char>
      <char style="xt" closed="false">
        <ref loc="EXO 3:15">Kuv 3:15</ref> </char>
      <char style="xo" closed="false">b </char>
      <char style="xt" closed="false">

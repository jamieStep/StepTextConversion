      <char style="xt" closed="false">
        <ref loc="PSA 118:9">Zab 118:9</ref> </char>
      <char style="xo" closed="false">b </char>
      <char style="xt" closed="false">
        <ref loc="ISA 2:22">Is 2:22</ref>
      </char>

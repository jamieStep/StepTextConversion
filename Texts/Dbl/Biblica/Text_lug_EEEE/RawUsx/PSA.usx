  <para style="q1">
    <verse number="9" style="v"/>
    <note caller="-" style="x">
      <char style="xo" closed="false">147:9 a </char>
      <char style="xt" closed="false">
        <ref loc="PSA 104:27-28">Zab 104:27-28</ref>; <ref loc="MAT 6:26">Mat 6:26</ref> </char>
